{
  "name": "imhere",
  "version": "0.1.0",
  "description": "A integrated node framework",
  "main": "app.js",
  "repository": "git@github.com:imheretw/imhere.git",
  "author": "ImHere <imhere.in.tw@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gulp serve",
    "test:core": "PORT=4000 node_modules/.bin/nyc node_modules/.bin/mocha --compilers js:babel-core/register -r test/init --recursive test src/**/*.spec.js -t 10000",
    "test:core:debug": "DEBUG_LEVEL=debug DEBUG=knex:query,knex:bindings,nock.* npm run test:core",
    "test": "NODE_ENV=test DEBUG_LEVEL=fatal npm run test:core",
    "test:debug": "NODE_ENV=test npm run test:core:debug",
    "test:mysql": "NODE_ENV=test_mysql npm run test:core",
    "test:mysql:debug": "NODE_ENV=test_mysql npm run test:core:debug",
    "db:create": "node db/create.js",
    "db:drop": "node db/drop.js",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "command:job:my": "./src/commands/job my",
    "eslint-pre-commit": "./scripts/eslint-pre-commit.sh",
    "jscs-pre-commit": "./scripts/jscs-pre-commit.sh"
  },
  "pre-commit": [
    "eslint-pre-commit",
    "jscs-pre-commit"
  ],
  "dependencies": {
    "async": "^2.1.2",
    "bcrypt": "^0.8.7",
