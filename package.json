  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "eslint-pre-commit": "./bin/eslint-pre-commit.sh"
}
