    "gulp-task-listing": "^1.0.1",
    "gulp-ttf2eot": "^1.1.1",
    "gulp-ttf2woff": "^1.1.0",
    "gulp-ttf2woff2": "^2.0.2",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.8",
    "main-bower-files": "^2.13.1",
    "mkdirp": "^0.5.1",
